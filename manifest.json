{
  "name": "InstaHoney",
  "version": "1.0",
  "description": "Socialize in Honeywell",
  "manifest_version": 2,
  "background": {
    "scripts": ["click.js"],
    "persistent" : true
  },
  "browser_action": {},
  "permissions": ["tabs", 
    "activeTab" ,
    "webRequest",
    "webRequestBlocking",
    "webNavigation",
    "*://*.instagram.com/*"
  ]
}